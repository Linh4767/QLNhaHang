@model IEnumerable<QLNhaHang.Models.LoaiMonAn>


@if (Model != null && Model.Any())
{
    <li class="nav-item">
        <button class="nav-link active" id="tab@Model.Count()" data-bs-toggle="tab" type="button" role="tab" data-maLoaiMon="Tất cả">
            Tất cả
        </button>
    </li>
    @for (int i = 0; i < Model.Count(); i++)
    {

        var item = Model.ElementAt(i);
        <li class="nav-item">
            <button class="nav-link" id="tab@i" data-bs-toggle="tab" type="button" role="tab" data-maLoaiMon="@item.MaLoaiMa">
                @item.TenLoaiMa
            </button>
        </li>
    }
}